<Window x:Class="Stripper.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Stripper"
        mc:Ignorable="d"
        Title="MainWindow" Height="850" Width="1000" Background="#FF2B2B2B">
    <Grid>
        
        <Image x:Name="ImageCode" Margin="10,10,85,10" Stretch="Fill"/>
        <Button x:Name="ButtonGenerate" Content="Button" Grid.Column="1" HorizontalAlignment="Right" Margin="0,10,10,0" VerticalAlignment="Top" Width="75" Click="ButtonGenerate_Click"/>

    </Grid>
</Window>
